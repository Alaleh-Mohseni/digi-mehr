{
	"plugins": ["prettier-plugin-tailwindcss"],
	"tailwindFunctions": ["clsx", "tw", "ctl"],
	"singleQuote": true,
	"trailingComma": "es5",
	"semi": false,
	"tabWidth": 2,
	"useTabs": true,
	"printWidth": 160,
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"endOfLine": "lf",
	"jsxSingleQuote": true,
	"htmlWhitespaceSensitivity": "strict",
	"quoteProps": "as-needed",
	"ignorePath": ".prettierignore",
	"embeddedLanguageFormatting": "auto"
}
